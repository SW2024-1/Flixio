<div class="users-container">
  <% @users.each do |e| %>
    <div class="user">
      <p>
        uid：<%= e.uid %><br>
        password： <%= e.password_digest %>
      </p>
    </div>
  <% end %>

  <div class="links">
    <%= link_to "新規追加", new_user_path, class: 'button register-btn' %>
    <%= link_to "ログイン", top_main_path, class: 'button login-btn' %>
  </div>
</div>
